{
  "expo": {
    "name": "PodcastGen",
    "slug": "podcastgen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "podcastgen",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.podcastgen.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#6B4CE6",
        "foregroundImage": "./assets/images/icon.png"
      },
      "package": "com.podcastgen.app",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#6B4CE6",
          "dark": {
            "backgroundColor": "#1A1A2E"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "a286cc99-a13d-45ce-ba3e-cb84ccfc08a8"
      },
      "googleClientId": "566269032122-40jjq2qhcf9bie8hu4jrncdfp3rqoneq.apps.googleusercontent.com",
      "googleAndroidClientId": "566269032122-t8kddv2o8t2p06kpbjdc2gjuoa5bdqir.apps.googleusercontent.com"
    }
  }
}
